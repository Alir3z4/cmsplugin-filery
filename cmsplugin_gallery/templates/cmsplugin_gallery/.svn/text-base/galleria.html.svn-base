<script type="text/javascript" src="{{ CMSPLUGIN_GALLERIA_MEDIA_URL }}galleria.js"></script>

<div id="{{ galleria.div_id }}" class="galleria">
	{% for picture in galleria.picture_set.all %}
		<img src="{{ picture.get_absolute_url }}" alt="{{ picture.caption }}" title="{{ picture.caption }}" />
		<!-- <p>{{ picture.caption }}</p> -->
	{% endfor %} 
</div>

<script type="text/javascript">
jQuery(function () {
	window.top.Galleria = Galleria; 
	Galleria.loadTheme('{{ CMSPLUGIN_GALLERIA_MEDIA_URL }}{{ galleria.theme }}');
	{{ constructor }}
});
</script>